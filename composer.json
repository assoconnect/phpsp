{
    "name": "assoconnect/phpsp",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/assoconnect/phpsp/"
        }
    ],
    "type": "library",
    "description": "PHP Abstraction lib for PSP services",
    "keywords": ["php", "psp"],
    "homepage": "https://github.com/assoconnect/phpsp/",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AssoConnect\\PHPSP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AssoConnect\\PHPSP\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2"
    },
    "require": {
        "php": "^7.4"
    }
}
